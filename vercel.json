{
    "version":2,
    "builds":[{"src":"./index.js", "use":"@vercel/node"}],
    "routes":[{"src":"/(.*)", "dest":"/"}],
    "headers": [
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ]

    }